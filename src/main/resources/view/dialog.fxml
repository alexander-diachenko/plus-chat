<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox fx:id="root" prefHeight="170.0" prefWidth="285.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="chat.controller.DialogController">
    <Pane maxHeight="35.0" minHeight="35.0" prefHeight="35.0" prefWidth="285.0"/>
    <GridPane prefHeight="135.0" prefWidth="285.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="181.0" minWidth="10.0" prefWidth="179.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="137.0" minWidth="10.0" prefWidth="105.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="215.0" minHeight="10.0" prefHeight="43.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="229.0" minHeight="0.0" prefHeight="46.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="116.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <HBox alignment="CENTER" prefHeight="45.0" prefWidth="142.5" spacing="20.0" GridPane.columnIndex="1"
              GridPane.rowIndex="2">
            <Button fx:id="confirm" onAction="#confirmAction" maxWidth="40.0" minWidth="40.0" mnemonicParsing="false" prefWidth="40.0"/>
            <Button fx:id="cancel" onAction="#cancelAction" maxWidth="40.0" minWidth="40.0" mnemonicParsing="false" prefWidth="40.0"/>
        </HBox>
        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="85.0" prefWidth="179.0"
               text="Приложение будет перезагружено." wrapText="true"/>
        <Label prefHeight="66.0" prefWidth="236.0" text="Продолжить?" GridPane.rowIndex="1"/>
        <Label fx:id="question" prefHeight="55.0" prefWidth="118.0" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    </GridPane>
</VBox>
