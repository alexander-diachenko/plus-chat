<?xml version="1.0" encoding="UTF-8"?>

<?import chat.component.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<ScrollPane prefHeight="365.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="chat.controller.settings.SettingDataTabController">
    <GridPane prefHeight="455.0" prefWidth="600.0" gridLinesVisible="true">
        <columnConstraints>
            <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS"/>
        </rowConstraints>
        <opaqueInsets>
            <Insets bottom="50.0" left="50.0" right="50.0" top="50.0"/>
        </opaqueInsets>
        <Label text="%data.commands">
            <GridPane.margin>
                <Insets left="100.0"/>
            </GridPane.margin>
        </Label>
        <Label text="%data.users" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets left="100.0"/>
            </GridPane.margin>
        </Label>
        <Label text="%data.ranks" GridPane.rowIndex="2">
            <GridPane.margin>
                <Insets left="100.0"/>
            </GridPane.margin>
        </Label>
        <Label text="%data.smiles" GridPane.rowIndex="3">
            <GridPane.margin>
                <Insets left="100.0"/>
            </GridPane.margin>
        </Label>
        <Label text="%data.directs" GridPane.rowIndex="4">
            <GridPane.margin>
                <Insets left="100.0"/>
            </GridPane.margin>
        </Label>
        <Label text="%data.orders" GridPane.rowIndex="5">
            <GridPane.margin>
                <Insets left="100.0"/>
            </GridPane.margin>
        </Label>
        <CustomButton fx:id="commandsData" onAction="#commandsDataAction" GridPane.columnIndex="1"
                      GridPane.halignment="CENTER" GridPane.rowIndex="0"/>
        <CustomButton fx:id="usersData" onAction="#usersDataAction" GridPane.columnIndex="1"
                GridPane.halignment="CENTER" GridPane.rowIndex="1"/>
        <CustomButton fx:id="ranksData" onAction="#ranksDataAction" GridPane.columnIndex="1"
                GridPane.halignment="CENTER" GridPane.rowIndex="2"/>
        <CustomButton fx:id="smilesData" onAction="#smilesDataAction" GridPane.columnIndex="1"
                GridPane.halignment="CENTER" GridPane.rowIndex="3"/>
        <CustomButton fx:id="directsData" onAction="#directsDataAction" GridPane.columnIndex="1"
                GridPane.halignment="CENTER" GridPane.rowIndex="4"/>
        <CustomButton fx:id="ordersData" onAction="#ordersDataAction" GridPane.columnIndex="1"
                GridPane.halignment="CENTER" GridPane.rowIndex="5"/>
    </GridPane>
</ScrollPane>