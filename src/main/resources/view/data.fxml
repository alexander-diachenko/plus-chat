<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<VBox fx:id="root" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="chat.controller.DataController">
    <Pane maxHeight="30.0" minHeight="30.0" prefHeight="30.0" prefWidth="200.0"/>
    <TableView fx:id="table" VBox.vgrow="ALWAYS">
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
    <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
        <Button fx:id="confirm" mnemonicParsing="false" onAction="#confirmAction"/>
        <Button fx:id="cancel" mnemonicParsing="false" onAction="#cancelAction"/>
        <HBox.margin>
            <Insets right="50.0"/>
        </HBox.margin>
    </HBox>
    <userData>
        <fx:reference source="controller"/>
    </userData>
</VBox>
