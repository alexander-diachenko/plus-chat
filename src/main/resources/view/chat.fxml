<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="root" alignment="TOP_CENTER" prefHeight="400.0" prefWidth="279.0" xmlns="http://javafx.com/javafx/8.0.121"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="chat.controller.ChatController">
    <Pane minHeight="30.0" prefHeight="30.0" maxHeight="30.0">
        <HBox fx:id="actions" layoutX="40">
            <Button fx:id="setting" onAction="#settingsOnAction" mnemonicParsing="false"/>
            <Button fx:id="onTop" onAction="#onTopOnAction" mnemonicParsing="false"/>
        </HBox>
    </Pane>
    <ScrollPane fx:id="scrollPane" fitToWidth="true">
        <VBox fx:id="container"/>
    </ScrollPane>
    <userData>
        <fx:reference source="controller"/>
    </userData>
</VBox>
